{
  "name": "@neuphonic/neuphonic-js",
  "version": "0.9.1",
  "description": "The official Neuphonic Typescript (Javascript) library ",
  "author": "Neuphonic Team",
  "repository": "https://github.com/neuphonic/neuphonic-js",
  "type": "module",
  "source": "src/index.ts",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/indexBrowser.d.ts",
      "import": "./dist/indexBrowser.js",
      "require": "./dist/indexBrowser.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "t": "tsx src/t.ts",
    "build": "rm -rf dist && rollup -c",
    "dev": "microbundle watch",
    "lint": "eslint --fix \"src/**/\"",
    "lint:check": "eslint \"src/**/\"",
    "test": "jest --verbose --runInBand --bail --setupFiles dotenv/config",
    "test:watch": "jest --watch",
    "test:ci": "jest --bail --runInBand --ci --setupFiles dotenv/config",
    "prettier": "prettier --write .",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.10",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "eventsource": "^3.0.5",
    "isomorphic-ws": "5.0.0",
    "js-base64": "^3.7.7",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
